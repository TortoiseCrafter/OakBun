  "files": [
    "dist"
  ],
  "license": "MIT",
  "scripts": {
    "build": "bun run build:main && bun run build:schema",
    "build:main": "tsdown",
    "build:schema": "tsdown --config tsdown.schema.ts",
    "dev": "tsdown --watch",
    "prepublish": "bun run build"
  },
  "type": "module",
  "types": "./dist/index.d.mts",
  "dependencies": {
    "@hono/zod-validator": "^0.7.5",
    "better-auth": "^1.4.5",
